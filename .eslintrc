{
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier",
    "plugin:import/recommended",
    "plugin:import/warnings",
    "plugin:tailwindcss/recommended"
  ],
  "plugins": ["tailwindcss"],
  "settings": {
    "tailwindcss": {
      "whitelist": [""]
    }
  },
  "rules": {
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ]
  },
  "ignorePatterns": ["next-sitemap.js"]
  //"semi":["error","always"]
  //}
}

{
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "dev": "next dev",
    "build": "next build && next-sitemap --config next-sitemap.js",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "eslint src --ext .js,jsx,.ts,.tsx --fix",
    "format": "prettier --write --ignore-path .gitignore 'src/**/*.{js,jsx,ts,tsx,json,css}'"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.0.7",
    "next": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@headlessui/react": "^1.4.2",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/react-scroll": "^1.8.3",
    "autoprefixer": "^10.2.6",
    "eslint": "^8.4.1",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-tailwindcss": "^3.0.3",
    "next-sitemap": "^1.6.203",
    "postcss": "^8.3.5",
    "prettier": "^2.5.1",
    "react-scroll": "^1.8.4",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.5.4"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100
  }
}
